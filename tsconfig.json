{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es2016",
    "jsx": "react",
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "removeComments": true
  }
}